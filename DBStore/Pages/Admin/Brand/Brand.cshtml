@page
@model DBStore.Pages.Admin.Brand.BrandModel
@{
    ViewData["Title"] = "Brand";
    Layout = "~/Pages/Shared/_LayoutAdmin.cshtml";
}

<div class="border backgroundWhite container">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Brand List </h2>

        </div>

        <div class="col-6 text-right">
            <a asp-page="Upsert" class="btn btn-primary"><i class="fas fa-plus">&nbsp; Create New </i></a>

        </div>
    </div>
    <br /> <br />

    <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>
                    Brand Logo
                </th>
                <th>
                    Brand Name
                </th>
                <th>
                    Display Order
                </th>
                <th></th>
            </tr>
        </thead>
    </table>


</div>

@section Scripts{
    <script src="~/js/Brand.js"></script>
}



