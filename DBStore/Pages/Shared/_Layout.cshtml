
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor
@using DBStore.Utility


<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Top Online Shopping site bd</title>
    <link href="https://fonts.googleapis.com/css?family=Hind:400,700" rel="stylesheet">
    <link type="text/css" rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/css/slick.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/css/slick-theme.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/css/nouislider.min.css" />
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    <link type="text/css" rel="stylesheet" href="~/Content/css/style.css" />
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-timepicker/1.10.0/jquery.timepicker.css" />

</head>
<body class="search-background">
    <div class="loader">
        <div class="inner"></div>
    </div>

    @await Html.PartialAsync("_Header")

    <div id="navigation">

        @await Html.PartialAsync("_Navbar")

    </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()

        </main>
    </div>

    @await Html.PartialAsync("_Footer")


    <script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/slick.min.js"></script>
    <script src="~/Content/js/nouislider.min.js"></script>
    <script src="~/Content/js/jquery.zoom.min.js"></script>
    @*<script src="~/Content/js/modernizr-2.6.2.js"></script>*@

    <script src="~/Content/js/main.js"></script>
    @*<script src="~/js/AppUtil.js"></script>
        <script src="~/js/notify.js"></script>*@
    <script src="https://kit.fontawesome.com/e19c476714.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery.serializeJSON/2.9.0/jquery.serializejson.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-timepicker/1.10.0/jquery.timepicker.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="https://cloud.tinymce.com/stable/tinymce.min.js?apiKey=g28lhnxtlihu4l4a381gw7tz47voznshwbi10lmsvlekrfme"></script>




    <script src="~/js/site.js" asp-append-version="true"></script>

    <script>
        $(document).ready(function () {

            $("#SerchDB").click(function (e) {
                e.preventDefault();
                var qrval = $("#productTilte").val();
                window.location.href = "/Customer/Product/Search?query=" + qrval + "";

            });
        });
    </script>


    @RenderSection("Scripts", required: false)
</body>
</html>
